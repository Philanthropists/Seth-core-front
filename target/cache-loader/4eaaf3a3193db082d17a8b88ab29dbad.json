KAAK,EAAC,EAAE,EAAC,GAAG,EAAC,GAAG,GAAG;wBAC1B,KAAK;4BACJ,CAAC,CAAC,KAAK,CAAC,GAAG,CAAC,WAAW,CAAC,EAAE,CAAC,CACvB,gCAAQ,KAAK,EAAE,WAAW,CAAC,EAAE,EAAE,GAAG,EAAE,WAAW,CAAC,EAAE,IAC/C,WAAW,CAAC,EAAE,CACR,CACV,CAAC;4BACJ,CAAC,CAAC,IAAI,CACA,CACF;gBACV,oBAAC,MAAM,IAAC,GAAG,EAAE,IAAI,EAAE,EAAE,EAAC,aAAa,EAAC,EAAE,EAAC,MAAM,EAAC,OAAO,QAAC,KAAK,EAAC,MAAM;oBAChE,oBAAC,eAAe,IAAC,IAAI,EAAC,YAAY,GAAG;;oBAErC,8BAAM,SAAS,EAAC,oBAAoB,WAAY,CACzC;;gBAET,oBAAC,MAAM,IAAC,KAAK,EAAC,SAAS,EAAC,EAAE,EAAC,aAAa,EAAC,IAAI,EAAC,QAAQ,EAAC,QAAQ,EAAE,QAAQ;oBACvE,oBAAC,eAAe,IAAC,IAAI,EAAC,MAAM,GAAG;kCAExB,CACF,CACV,CACG,CACF,CACF,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,MAAM,eAAe,GAAG,CAAC,UAAsB,EAAE,EAAE,CAAC,CAAC;IACnD,KAAK,EAAE,UAAU,CAAC,IAAI,CAAC,QAAQ;IAC/B,SAAS,EAAE,UAAU,CAAC,GAAG,CAAC,MAAM;IAChC,OAAO,EAAE,UAAU,CAAC,GAAG,CAAC,OAAO;IAC/B,QAAQ,EAAE,UAAU,CAAC,GAAG,CAAC,QAAQ;IACjC,aAAa,EAAE,UAAU,CAAC,GAAG,CAAC,aAAa;CAC5C,CAAC,CAAC;AAEH,MAAM,kBAAkB,GAAG;IACzB,QAAQ;IACR,SAAS;IACT,YAAY;IACZ,YAAY;IACZ,KAAK;CACN,CAAC;AAKF,eAAe,OAAO,CAAC,eAAe,EAAE,kBAAkB,CAAC,CAAC,SAAS,CAAC,CAAC","sourcesContent":["import React, { useState, useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport { Link, RouteComponentProps } from 'react-router-dom';\nimport { Button, Row, Col, Label } from 'reactstrap';\nimport { AvFeedback, AvForm, AvGroup, AvInput, AvField } from 'availity-reactstrap-validation';\nimport { ICrudGetAction, ICrudGetAllAction, ICrudPutAction } from 'react-jhipster';\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { IRootState } from 'app/shared/reducers';\n\nimport { IPost } from 'app/shared/model/post.model';\nimport { getEntities as getPosts } from 'app/entities/post/post.reducer';\nimport { getEntity, updateEntity, createEntity, reset } from './tag.reducer';\nimport { ITag } from 'app/shared/model/tag.model';\nimport { convertDateTimeFromServer, convertDateTimeToServer, displayDefaultDateTime } from 'app/shared/util/date-utils';\nimport { mapIdList } from 'app/shared/util/entity-utils';\n\nexport interface ITagUpdateProps extends StateProps, DispatchProps, RouteComponentProps<{ id: string }> {}\n\nexport const TagUpdate = (props: ITagUpdateProps) => {\n  const [postId, setPostId] = useState('0');\n  const [isNew, setIsNew] = useState(!props.match.params || !props.match.params.id);\n\n  const { tagEntity, posts, loading, updating } = props;\n\n  const handleClose = () => {\n    props.history.push('/tag');\n  };\n\n  useEffect(() => {\n    if (isNew) {\n      props.reset();\n    } else {\n      props.getEntity(props.match.params.id);\n    }\n\n    props.getPosts();\n  }, []);\n\n  useEffect(() => {\n    if (props.updateSuccess) {\n      handleClose();\n    }\n  }, [props.updateSuccess]);\n\n  const saveEntity = (event, errors, values) => {\n    if (errors.length === 0) {\n      const entity = {\n        ...tagEntity,\n        ...values,\n      };\n\n      if (isNew) {\n        props.createEntity(entity);\n      } else {\n        props.updateEntity(entity);\n      }\n    }\n  };\n\n  return (\n    <div>\n      <Row className=\"justify-content-center\">\n        <Col md=\"8\">\n          <h2 id=\"solidaridadApp.tag.home.createOrEditLabel\">Create or edit a Tag</h2>\n        </Col>\n      </Row>\n      <Row className=\"justify-content-center\">\n        <Col md=\"8\">\n          {loading ? (\n            <p>Loading...</p>\n          ) : (\n            <AvForm model={isNew ? {} : tagEntity} onSubmit={saveEntity}>\n              {!isNew ? (\n                <AvGroup>\n                  <Label for=\"tag-id\">ID</Label>\n                  <AvInput id=\"tag-id\" type=\"text\" className=\"form-control\" name=\"id\" required readOnly />\n                </AvGroup>\n              ) : null}\n              <AvGroup>\n                <Label id=\"nameLabel\" for=\"tag-name\">\n                  Name\n                </Label>\n                <AvField\n                  id=\"tag-name\"\n                  type=\"text\"\n                  name=\"name\"\n                  validate={{\n                    required: { value: true, errorMessage: 'This field is required.' },\n                  }}\n                />\n              </AvGroup>\n              <AvGroup>\n                <Label for=\"tag-post\">Post</Label>\n                <AvInput id=\"tag-post\" type=\"select\" className=\"form-control\" name=\"post.id\">\n                  <option value=\"\" key=\"0\" />\n                  {posts\n                    ? posts.map(otherEntity => (\n                        <option value={otherEntity.id} key={otherEntity.id}>\n                          {otherEntity.id}\n                        </option>\n                      ))\n                    : null}\n                </AvInput>\n              </AvGroup>\n              <Button tag={Link} id=\"cancel-save\" to=\"/tag\" replace color=\"info\">\n                <FontAwesomeIcon icon=\"arrow-left\" />\n                &nbsp;\n                <span className=\"d-none d-md-inline\">Back</span>\n              </Button>\n              &nbsp;\n              <Button color=\"primary\" id=\"save-entity\" type=\"submit\" disabled={updating}>\n                <FontAwesomeIcon icon=\"save\" />\n                &nbsp; Save\n              </Button>\n            </AvForm>\n          )}\n        </Col>\n      </Row>\n    </div>\n  );\n};\n\nconst mapStateToProps = (storeState: IRootState) => ({\n  posts: storeState.post.entities,\n  tagEntity: storeState.tag.entity,\n  loading: storeState.tag.loading,\n  updating: storeState.tag.updating,\n  updateSuccess: storeState.tag.updateSuccess,\n});\n\nconst mapDispatchToProps = {\n  getPosts,\n  getEntity,\n  updateEntity,\n  createEntity,\n  reset,\n};\n\ntype StateProps = ReturnType<typeof mapStateToProps>;\ntype DispatchProps = typeof mapDispatchToProps;\n\nexport default connect(mapStateToProps, mapDispatchToProps)(TagUpdate);\n"]}]}